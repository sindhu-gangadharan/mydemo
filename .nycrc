{
  "check-coverage": true,
  "per-file": true,
  "lines": 80,
  "statements": 80,
  "functions": 80,
  "branches": 80,
  "include": [
    "src/**/*.js"
  ],
  "reporter": [
    "text",
    "html"
  ],
  "extension": [
    ".js"
  ],
  "cache": false,
  "all": true,
  "report-dir": "./reports/coverage"
}
